<p id="notice"><%= notice %></p>

<h1>Listing Course To Homeworks</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Homework</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_to_homeworks.each do |course_to_homework| %>
      <tr>
        <td><%= course_to_homework.course %></td>
        <td><%= course_to_homework.homework %></td>
        <td><%= link_to 'Show', course_to_homework %></td>
        <td><%= link_to 'Edit', edit_course_to_homework_path(course_to_homework) %></td>
        <td><%= link_to 'Destroy', course_to_homework, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course to homework', new_course_to_homework_path %>
